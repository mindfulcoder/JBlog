<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yoolean.jblog.dao.AdminUserMapper">
    <resultMap id="AdminUserMap" type="com.yoolean.jblog.entity.AdminUser">
        <id column="admin_user_id" jdbcType="INTEGER" property="adminUserId"/>
        <result column="login_user_name" jdbcType="VARCHAR" property="loginUserName"/>
        <result column="login_password" jdbcType="VARCHAR" property="loginPassword"/>
        <result column="nick_name" jdbcType="INTEGER" property="nickName"/>
        <result column="locked" jdbcType="TINYINT" property="locked"/>
    </resultMap>
    <insert id="insert" parameterType="com.yoolean.jblog.entity.AdminUser" useGeneratedKeys="true"
            keyProperty="adminUserId">
        insert into tb_admin_user(admin_user_id, login_user_name, login_password, nick_name, locked)
        values (#{adminUserId}, #{loginUserName}, #{loginPassword}, #{nickName}, #{locked});
    </insert>
    <insert id="insertSelective"></insert>
    <update id="updateByPrimaryKeySelective"></update>
    <update id="updateByPrimaryKey"></update>
    <select id="login" resultType="com.yoolean.jblog.entity.AdminUser"></select>
    <select id="selectByPrimaryKey" resultType="com.yoolean.jblog.entity.AdminUser"></select>

</mapper>